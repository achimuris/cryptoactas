version: "3.3"

services:
  backend:
    build:
      context: ./server
      dockerfile: ./Dockerfile
    image: backend/cryptoactas
    ports:
      - "8080:8080"

  frontend:
    env_file:
      "./client/.env"
    build:
      context: ./client
      dockerfile: ./Dockerfile
    image: "frontend/cryptoactas"
    ports:
      - "8081:8081"
    links:
      - "backend:be"
